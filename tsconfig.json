{
  "compilerOptions": {
    "declaration": false,
    "importHelpers": false,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "outDir": "build",
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "rxjs-complementary": ["./src/index"],
      "rxjs-complementary/operators": ["./src/operators/index"]
    }
  },
  "exclude": ["node_modules"],
  "include": [
    "src/**/*.ts"
  ]
}
